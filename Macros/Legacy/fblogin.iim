VERSION BUILD=7601105 RECORDER=FX
TAB T=1
SET !TIMEOUT_PAGE 30
URL GOTO=https://www.facebook.com
TAG POS=1 TYPE=INPUT:TEXT FORM=ID:login_form ATTR=ID:email CONTENT={{login}}
SET !ENCRYPTION NO
TAG POS=1 TYPE=INPUT:PASSWORD FORM=ID:login_form ATTR=ID:pass CONTENT={{password}}
WAIT SECONDS=1
SET !ERRORIGNORE YES
SET !TIMEOUT_STEP 1
TAG POS=1 TYPE=INPUT:SUBMIT FORM=ID:login_form ATTR=ID:u_*
TAG POS=1 TYPE=BUTTON FORM=ID:login_form ATTR=ID:loginbutton
WAIT SECONDS=1
